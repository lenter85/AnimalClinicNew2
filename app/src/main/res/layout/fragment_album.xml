<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.myapplication.diary.AlbumFragment">

    <ImageView
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:id="@+id/imageView29"
        android:background="@drawable/dog05" />

    <ImageView
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:id="@+id/imageView30"
        android:background="@drawable/dog04_small"
        android:layout_above="@+id/gridViewAlbum"
        android:layout_toRightOf="@+id/imageView29"
        android:layout_toEndOf="@+id/imageView29" />

    <GridView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/gridViewAlbum"
        android:padding="3dp"
        android:layout_below="@+id/imageView29"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
