<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.myapplication.diary.DiaryFragment">

    <!-- TODO: Update blank fragment layout -->


        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/listViewDiary"
            android:layout_alignRight="@+id/imgAddDiary"
            android:layout_alignEnd="@+id/imgAddDiary"
            android:background="@color/material_grey_300"
            android:dividerHeight="2dp"
            android:divider="#ffffff" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab"
        android:layout_gravity="bottom|right"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        app:backgroundTint="#53f219"
        android:src="@drawable/ic_add_entry" />

    <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:id="@+id/imgAddDiary"
            android:layout_marginBottom="44dp"
            android:background="@drawable/add_btn"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:onClick="onClickToRegisterMyDiary"
            android:src="@drawable/ic_add_entry"
            android:layout_gravity="right|bottom"
        android:visibility="invisible" />

</FrameLayout>
